class c:
    def __init__(self, list):
        c.self = self
        c.list = list
    def size(list):
          len(list)
    def avg(list):
          global a
          a = 0
          for i in list:
            a = a + 1
          a/list.size()
data = [23,45,67,24,57,98,35,58,64,83]
c(data)
print("Data has "+ str(c.size(data)) + " values.")
